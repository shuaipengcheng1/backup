<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.seata_order.Dao.OrderDao">
    <insert id="CreateOrder" >
        INSERT into `order` (id,order_info,order_state) VALUES  (#{id},#{order_info},0)
  </insert>
  <update id="UpdateOrder">
      Update `order` Set order_state=#{arg0} Where id = #{arg1}
  </update>
</mapper>