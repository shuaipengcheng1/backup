<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.day2.pro2.Dao.PayDao">
    <resultMap id="x" type="com.day2.pro2.Domain.Payment">
        <result property="Detail" column="String"/>
        <result property="id" column="id"/>
    </resultMap>
    <select id="Select" resultMap="x">
        select *
        from payment
        where id = #{id};
  </select>
    <insert id="Create">
        insert into payment (id,String)
        values (#{id},#{Detail});
    </insert>
</mapper>