<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mi.login_submit.Dao.LoginSubmit">
    <select id="Login" resultType="com.mi.login_submit.Domain.User">
        select *
        from user
        where username=#{arg0} and password=#{arg1}
  </select>
    <insert id="Submit">
        insert into user (password,username) values (#{arg1},#{arg0})
    </insert>
    <select id="Check_Name" resultType="com.mi.login_submit.Domain.User">
        select * from user where username=#{arg0}
    </select>
</mapper>